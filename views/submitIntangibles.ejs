<div class="row mt-5">
    <div class="col-md-6 m-auto">
      <div class="card card-body">
        <h1 class="text-center mb-3"><i class="fas fa-sign-in-alt"></i> Submit Position Specific Intangibles</h1>
        <%- include('./partials/messages'); %>
        <h3>Submit the Intangibles that you would like to grade players on. These Intangibles will be position specific.</h3>
        <form action="/stats/addIntang" method="POST">
          <div class="form-group">
            <label for="pos">Position</label>
            <input type="pos" id="pos" name="pos" class="form-control" placeholder="Enter Position"/>
          </div>
          <div class="form-group">
            <label for="i1">Intangible 1</label>
            <input type="i1" id="i1" name="i1" class="form-control" placeholder="Enter Intangible Name"/>
          </div>
          <div class="form-group">
            <label for="i2">Intangible 2</label>
            <input type="i2" id="i2" name="i2" class="form-control" placeholder="Enter Intangible Name"/>
          </div>
          <div class="form-group">
            <label for="i3">Intangible 3</label>
            <input type="i3" id="i3" name="i3" class="form-control" placeholder="Enter Intangible Name"/>
          </div>
          <div class="form-group">
            <label for="i4">Intangible 4</label>
            <input type="i4" id="i4" name="i4" class="form-control" placeholder="Enter Intangible Name"/>
          </div>
          
          <button type="submit" class="btn btn-primary btn-block">Submit Intangibles</button>
        </form>
        <p class="lead mt-4">
          Done? <a href="/index/coachHome">Pratice Stats Page</a>
        </p>
      </div>
    </div>
  </div>